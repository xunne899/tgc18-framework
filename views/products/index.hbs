
{{#extends 'base'}}

{{#block 'content'}}
<table class="table">
    <thead>
        <tr>
            <th>Title</th>
                <th>Cost</th>
            <th>Description</th>        
              <th>Date</th>
                <th>Stock</th>
               <th>Height</th>
                <th>Width</th>
        
        </tr>
    </thead>
    <tbody>
        {{#each products}}
        <tr>
            <td>{{this.title}}</td>
             <td>{{this.cost}}</td>
            <td>{{this.description}}</td>
             <td>{{this.date}}</td>
              <td>{{this.stock}}</td>
               <td>{{this.height}}</td>
                 <td>{{this.width}}</td>
           
        </tr>
        {{/each}}
    </tbody>
</table>
{{/block}}